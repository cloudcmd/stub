  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^6.2.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-putout": "^2.0.0",
    "madrun": "^2.5.0",
    "mock-require": "^3.0.2",
    "nodemon": "^1.14.12",
    "nyc": "^14.1.1",
    "putout": "^5.8.0",
    "redrun": "^7.0.2",
    "try-catch": "^2.0.0",
    "try-to-catch": "^1.1.1"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.3.0"
  },
  "publishConfig": {
    "access": "public"
